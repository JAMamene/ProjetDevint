<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="dvt.polybeatmaker.controller.ItemChooserController">
    <children>
        <VBox alignment="CENTER">
            <children>
                <ScrollPane hbarPolicy="NEVER" prefHeight="700.0" prefWidth="1200.0">
                    <content>
                        <VBox fx:id="itemBox" alignment="CENTER" prefWidth="1200.0" spacing="10.0">
                            <children>
                                <Label fx:id="mainLabel" minHeight="-Infinity" minWidth="-Infinity" prefHeight="130.0" prefWidth="1200.0">
                                    <VBox.margin>
                                        <Insets bottom="30.0" />
                                    </VBox.margin>
                                </Label>
                            </children>
                        </VBox>
                    </content>
                    <VBox.margin>
                        <Insets bottom="10.0" />
                    </VBox.margin>
                </ScrollPane>
                <Button fx:id="quit" mnemonicParsing="false" onAction="#exit" prefHeight="100.0" prefWidth="700.0" text="QUITTER">
                    <VBox.margin>
                        <Insets bottom="30.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
